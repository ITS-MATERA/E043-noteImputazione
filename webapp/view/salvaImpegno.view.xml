<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:u="sap.ui.unified"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:f="sap.f"
    xmlns:core="sap.ui.core"
    xmlns:menu="sap.m.MenuItem"
     xmlns:zcontrolPosFin="zsap.com.r3.cobi.s4.custposfin.z_s4_zposfinlink.controls"  
    xmlns:zcontrollinkSar="custlinksar.custlinksar.controls" 
    height="100%" controllerName="project1.controller.salvaImpegno">
    <f:DynamicPage id="dynamicPageId" showFooter="true">
        <f:header>
            <f:DynamicPageHeader pinnable="false">
                <layout:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0">
                    <!-- 1 riga -->
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="numNI" text="{i18n>ProgressivoNI}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="numNI1" text="{EntityModel>/Header/ZchiaveNi}" wrapping="true" />
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox>
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="dataReg" text="{i18n>Data} NI: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="dataReg1"  text="{path:'EntityModel>/Header/ZdataCreaz', formatter:'.formatter.convert'}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox>
                    <!-- 2 riga -->
                        <FlexBox alignItems="Start" justifyContent="Start" class="text-overflow">
                                <Label id="SARW2" text="{i18n>SAR}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <zcontrollinkSar:LinkSar id="linkSarBilancio" anno="{EntityModel>/Header/Esercizio}" eos="E" fikrs="S001" fase="GEST" SEM_OBJ="" reale="R" 
                                    versione="D" bilancio="" fictr="{EntityModel>/Header/Fistl}" datbis="99991231" text="{EntityModel>/Header/Fistl}"
                                    wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" linebreak="true" /></layoutData>
                        </FlexBox>    
                        <FlexBox alignItems="Start" justifyContent="Start" class="text-overflow">
                                <Label id="pos_FinW2" text="{i18n>PF}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <zcontrolPosFin:LinkPosizioneFinanziaria id="linkPosFinSpesa" anno="{EntityModel>/Header/Esercizio}" eos="S" fikrs="S001" fase="GEST" reale="R" 
                                    versione="D" fipex="{EntityModel>/Header/Fipex}" datbis="99991231" text="{EntityModel>/Header/Fipex}" semanticObject="ESAMINAMODIFICA"
                                    wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox>  
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="oggSpesa" text="{i18n>oggSpesa}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="oggSpesa1" text="{EntityModel>/Header/ZoggSpesa}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox>  
                    <!-- 3 riga -->
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="mese" text="{i18n>Mese}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="mese1" text="{path: 'EntityModel>/Header/Zmese', formatter:'.formatter.getMonthName'}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" linebreak="true" /></layoutData>
                        </FlexBox>    
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="comp" text="{i18n>comp}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="comp1" wrapping="true" 
                                    text="{= ${EntityModel>/Header/ZcompRes} === 'C' ? 'Competenza' : ${EntityModel>/Header/ZcompRes} === 'R' ? 'Residui' : ''}"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox> 
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="statoNI" text="{i18n>statoNI}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="statoNI1" text="{EntityModel>/Header/ZcodiStatoniDesc}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox> 
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="importoTot" text="{i18n>importoTot}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="importoTot1" text="{path: 'EntityModel>/Header/ZimpoTotni', formatter:'.formatter.convertFormattedNumber'}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox>  
                    <!-- 4 riga -->
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="Lifnr" text="{i18n>Lifnr}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="Lifnr1" text="{EntityModel>/ImpegnoValues/ImpegnoValues_Lifnr}" wrapping="true" />
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" linebreak="true" /></layoutData>
                        </FlexBox>  
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="Nome" text="{i18n>Nome}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="Nome1" text="{EntityModel>/ImpegnoValues/ImpegnoValues_Nome}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox> 
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="Zwels" text="{i18n>Zwels}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="Zwels1" text="{EntityModel>/ImpegnoValues/ImpegnoValues_ZwelsDesc}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                        </FlexBox>
                    <!-- 5 riga -->
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="CentroCosto" text="Centro di Costo: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="CentroCosto1" text="{EntityModel>/ImpegnoValues/ImpegnoValues_Cdc}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" linebreak="true" /></layoutData>
                        </FlexBox> 
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="ConCoGe" text="Conto Co.Ge.: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="ConCoGe1" text="{EntityModel>/ImpegnoValues/ImpegnoValues_Coge}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12"  /></layoutData>
                        </FlexBox>
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="ImpLiq" text="Importo Liquidazione: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="ImpLiq1" text="{path: 'EntityModel>/Header/ZimpoTotni', formatter:'.formatter.convertFormattedNumber'}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12"  /></layoutData>
                        </FlexBox> 
                    <!-- 6 riga -->
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="CodiceGes" text="{i18n>Zcodgest}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="CodiceGes1" text="{EntityModel>/ImpegnoValues/ImpegnoValues_CodiceGestionale}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12" linebreak="true" /></layoutData>
                        </FlexBox> 
                        <FlexBox alignItems="Start" justifyContent="Start">
                                <Label id="CausalePagamento" text="{i18n>Zcauspag}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                                <Label id="CausalePagamento1" text="{EntityModel>/ImpegnoValues/ImpegnoValues_CausalePagamento}" wrapping="true"/>
                            <layoutData><layout:GridData  span="XL3 L3 M12 S12"  /></layoutData>
                        </FlexBox>

                    <!-- 7 riga -->  
                    
                    <FlexBox alignItems="Start" justifyContent="Start">
                            <Label id="DataProtocollo" text="{i18n>DataProtocollo}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                            <!-- <Label id="DataProtocollo1" wrapping="true" 
                                text="{path:'EntityModel>/ImpegnoValues/ImpegnoValues_DataProtocollo', formatter:'.formatter.convert'}"/> -->
                            <Label id="DataProtocollo1" wrapping="true" 
                                text="{EntityModel>/ImpegnoValues/ImpegnoValues_DataProtocollo_Header}"/>
                        <layoutData><layout:GridData  span="XL3 L3 M12 S12" linebreak="true" /></layoutData>
                    </FlexBox>  
                    <FlexBox alignItems="Start" justifyContent="Start">
                            <Label id="NProtocollo" text="{i18n>NProtocollo}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                            <Label id="NProtocollo1" wrapping="true" text="{EntityModel>/ImpegnoValues/ImpegnoValues_NProtocollo}"/>
                        <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                    </FlexBox>  
                    <FlexBox alignItems="Start" justifyContent="Start">
                            <Label id="CodiceGestionale" text="{i18n>CodiceGestionale}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                            <Label id="CodiceGestionale1" wrapping="true" text="{EntityModel>/ImpegnoValues/ImpegnoValues_CodiceGestionale}"/>
                        <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                    </FlexBox> 
                    <FlexBox alignItems="Start" justifyContent="Start">
                            <Label id="DescrizioneCodGest" text="{i18n>DescrizioneCodGest}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                            <Label id="DescrizioneCodGest1" wrapping="true" text="{EntityModel>/ImpegnoValues/ImpegnoValues_Descrizione}"/>
                        <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                    </FlexBox> 

                    <!-- 8 riga -->                    
                    <FlexBox alignItems="Start" justifyContent="Start">
                            <Label id="LocPagamento" text="{i18n>LocPagamento}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                            <Label id="LocPagamento1" wrapping="true" text="{EntityModel>/ImpegnoValues/ImpegnoValues_LocalitaPagamento}"/>
                        <layoutData><layout:GridData  span="XL3 L3 M12 S12" linebreak="true" /></layoutData>
                    </FlexBox>  
                    <FlexBox alignItems="Start" justifyContent="Start">
                            <Label id="DataEsigibilita" text="{i18n>DataEsigibilita}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                            <!-- <Label id="DataEsigibilita1" wrapping="true" 
                                text="{path:'EntityModel>/ImpegnoValues/ImpegnoValues_DataEsigibilita', formatter:'.formatter.convert'}"/> -->
                            <Label id="DataEsigibilita1" wrapping="true" 
                                text="{EntityModel>/ImpegnoValues/ImpegnoValues_DataEsigibilita_Header}"/>
                        <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                    </FlexBox> 
                    <FlexBox alignItems="Start" justifyContent="Start">
                            <Label id="ZonaIntervento" text="{i18n>ZonaIntervento}: " design="Bold" class="lbl-header-margin-right" wrapping="true"/>
                            <Label id="ZonaIntervento1" wrapping="true" text="{EntityModel>/ImpegnoValues/ImpegnoValues_ZonaIntervento}"/>
                        <layoutData><layout:GridData  span="XL3 L3 M12 S12" /></layoutData>
                    </FlexBox> 
                </layout:Grid>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <IconTabBar id="salvaImpegnoITB" class="sapUiResponsiveContentPadding" select="onSelect">
                <items>
                    <IconTabFilter icon="sap-icon://detail-view" text="Dettaglio NI" key="dettagliNI">
                        <core:Fragment fragmentName="project1.fragment.salvaImpegno" type="XML" />
                    </IconTabFilter>
                    <IconTabFilter icon="sap-icon://history" text="Workflow" key="Workflow">
                        <core:Fragment fragmentName="project1.fragment.workflow" type="XML"/>
                    </IconTabFilter>
                    <IconTabFilter icon="sap-icon://documents" text="Fascicolo" key="Fascicolo">
                    </IconTabFilter>
                </items>
            </IconTabBar>

        </f:content>
        <f:footer>
            <OverflowToolbar>
                <Button text="Indietro" type="Emphasized" press="onBackButton" />
                <ToolbarSpacer />
                <Button type="Emphasized" text="Annulla NI" press="onCancelNI" id="AnnullaNI" />
                <Button type="Emphasized" text="Completa NI" press="onCompleteNI" id="CompletaNI" />
            </OverflowToolbar>
        </f:footer>
    </f:DynamicPage>
</mvc:View>