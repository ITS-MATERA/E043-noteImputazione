            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <IconTabBar id="annullataNIITB" class="sapUiResponsiveContentPadding" select="onSelect">
                <items>
                    <IconTabFilter icon="sap-icon://detail-view" text="Dettaglio NI" key="dettagliNI">
                        <core:Fragment fragmentName="project1.fragment.annullataNI" type="XML" />
                    </IconTabFilter>
                    <IconTabFilter icon="sap-icon://history" text="Workflow" key="Workflow">
                        <core:Fragment fragmentName="project1.fragment.workflow" type="XML"/>
                    </IconTabFilter>
                    <IconTabFilter id="idFascicoloIconTabFilter" icon="sap-icon://documents" text="Fascicolo" key="Fascicolo" enabled="false">
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </f:content>
        <f:footer>
            <OverflowToolbar>
                <Button text="Indietro" type="Emphasized" press="onBackButton" />
            </OverflowToolbar>
        </f:footer>
